      call blacs_get
      call pdgesv
      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"; then :
  success=yes
else
  success=no
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: $success" >&5
$as_echo "$success" >&6; }
    test $success = yes && break
