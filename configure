### HDF5 ###


hdf5_guess()
{
  h5fc_=$(which h5fc 2> /dev/null)
  test "$h5fc_" || return 1
  hdf5_root=$(expr "$h5fc_" : '\(.*\)/bin/h5fc')
  test "$hdf5_root" || return 1
}


# Check whether --with-hdf5 was given.
if test "${with_hdf5+set}" = set; then :
  withval=$with_hdf5;
else
  with_hdf5=
fi

test "$with_hdf5" && FCFLAGS="$FCFLAGS -I$with_hdf5/include"
test "$with_hdf5" && LDFLAGS="$LDFLAGS -L$with_hdf5/lib"

LIBS="$LIBS -lhdf5 -lhdf5_fortran"
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for HDF5" >&5
$as_echo_n "checking for HDF5... " >&6; }
cat > conftest.$ac_ext <<_ACEOF
      program main
      use hdf5
      call h5open_f(i)
      end
_ACEOF
if ac_fn_fc_try_compile "$LINENO"; then :
  success=yes
else
  success="compilation error"
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $success" >&5
$as_echo "$success" >&6; }
if test "$success" = "compilation error"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for HDF5 include directory in CPATH" >&5
$as_echo_n "checking for HDF5 include directory in CPATH... " >&6; }
  if test "$CPATH"; then
    HDF5_INCLUDE="$(expr $CPATH : '.*:\([^:]*HDF5[^:]*\):')"
    test "$HDF5_INCLUDE" || HDF5_INCLUDE="$(expr $CPATH : '\([^:]*HDF5[^:]*\):')"
    test "$HDF5_INCLUDE" || HDF5_INCLUDE="$(expr $CPATH : '.*:\([^:]*HDF5[^:]*\)$')"
    test "$HDF5_INCLUDE" || HDF5_INCLUDE="$(expr $CPATH : '\([^:]*HDF5[^:]*\)$')"
  fi
  if test "$HDF5_INCLUDE"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: $HDF5_INCLUDE" >&5
$as_echo "$HDF5_INCLUDE" >&6; }
    FCFLAGS="$FCFLAGS -I$HDF5_INCLUDE"
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking if $HDF5_INCLUDE contains hdf5.mod" >&5
$as_echo_n "checking if $HDF5_INCLUDE contains hdf5.mod... " >&6; }
    cat > conftest.$ac_ext <<_ACEOF
      program main
      use hdf5
          call h5open_f(i)
      end
_ACEOF
if ac_fn_fc_try_compile "$LINENO"; then :
  success=yes
else
  success="no"
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
    test "$success" == "no" && HDF5_INCLUDE=''
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: $success" >&5
$as_echo "$success" >&6; }
  else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
  fi
  if ! test "$HDF5_INCLUDE"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking if h5fc exists and wraps $compnm" >&5
$as_echo_n "checking if h5fc exists and wraps $compnm... " >&6; }
    if hdf5_guess; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
      FCFLAGS="$FCFLAGS -I$hdf5_root/include"
      { $as_echo "$as_me:${as_lineno-$LINENO}: checking if $hdf5_root/include can be used" >&5
$as_echo_n "checking if $hdf5_root/include can be used... " >&6; }
      cat > conftest.$ac_ext <<_ACEOF
      program main
      use hdf5
          call h5open_f(i)
      end
_ACEOF
if ac_fn_fc_try_compile "$LINENO"; then :
  success=yes
else
  success="no"
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: $success" >&5
$as_echo "$success" >&6; }
      if test "$success" == no; then
        { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Could not compile with HDF5. Specify HDF5 root directory: --with-hdf5=DIR
See \`config.log' for more details" "$LINENO" 5; }
      fi
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Could not compile with HDF5. Specify HDF5 root directory: --with-hdf5=DIR
